ALTER TABLE `tbl_users` ADD `status` INT NOT NULL COMMENT '3 - added by staff , 2 - forwarded by manager, 1 - approved by admin' AFTER `parent_id` ;

UPDATE `expocad`.`tbl_users` SET `status` = '1' WHERE `tbl_users`.`user_id` = 1;
UPDATE `expocad`.`tbl_users` SET `status` = '1' WHERE `tbl_users`.`user_id` = 2;
UPDATE `expocad`.`tbl_users` SET `status` = '1' WHERE `tbl_users`.`user_id` = 3;
UPDATE `expocad`.`tbl_users` SET `status` = '2' WHERE `tbl_users`.`user_id` = 4;

